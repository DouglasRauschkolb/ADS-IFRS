programa
{
	funcao inicio()
	{
		inteiro num1, num2
		escreva ("Escreva um numero: ")
		leia(num1)
		escreva ("Escreva um numero: ")
		leia(num2)
		escreva("O produto dos dois números é: ", num1*num2, "\n")
	}
}
